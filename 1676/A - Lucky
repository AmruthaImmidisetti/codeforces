import java.util.*;import java.lang.*;import java.io.*;Â public class Luckyy{    public static void main(String[] args) throws java.lang.Exception    {        Scanner sc = new Scanner(System.in);        int t;        t = sc.nextInt();        while (t > 0) {            String s = sc.next();            int sum = 0, res = 0, f = 0;            for (int i = 0; i < s.length(); i++) {                sum += (s.charAt(i) - '0');                if ((i + 1) % 3 == 0) {                    if (res == sum) {                        f = 1;                        break;                    }                    res = sum;                    sum = 0;                }            }            if (f == 1) System.out.println("YES");            else System.out.println("NO");            t--;        }    }}