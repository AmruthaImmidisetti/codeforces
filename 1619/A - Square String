#include <bits/stdc++.h>using namespace std;Â int main() {    int t;    cin >> t;    while (t--) {        string s;        cin >> s;        if (s.size() % 2 == 1) {            cout << "NO" << endl;        } else {            int half = s.size() / 2;            string s1 = s.substr(0, half);            string s2 = s.substr(half, s.size() - half);            if (s1 == s2)                cout << "YES" << endl;            else                cout << "NO" << endl;        }    }    return 0;}